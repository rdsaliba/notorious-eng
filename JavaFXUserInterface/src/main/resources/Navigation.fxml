<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="834.0" prefWidth="58.0" styleClass="nav"
            stylesheets="@css/navigation.css" xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="controllers.NavigationController">
    <children>
        <VBox prefHeight="251.0" prefWidth="58.0"/>
        <VBox layoutY="256.0" prefHeight="306.0" prefWidth="58.0">
            <children>
                <Button fx:id="assetMenuBtn" contentDisplay="TOP" mnemonicParsing="false" prefHeight="58.0"
                        prefWidth="58.0" styleClass="navBtn">
                    <graphic>
                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@imgs/outline_dvr_white_18dp.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="assetTypeMenuBtn" contentDisplay="TOP" mnemonicParsing="false" prefHeight="58.0"
                        prefWidth="58.0" styleClass="navBtn">
                    <graphic>
                        <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@imgs/outline_type_white.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </children>
        </VBox>
        <VBox layoutY="683.0" prefHeight="144.0" prefWidth="58.0" AnchorPane.bottomAnchor="7.0">
            <children>
                <Button fx:id="exitMenuBtn" contentDisplay="TOP" mnemonicParsing="false" prefHeight="58.0"
                        prefWidth="58.0" styleClass="navBtn">
                    <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@imgs/outline_exit_to_app_white_36dp.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
